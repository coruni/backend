<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.TypeApi.dao.Reward_logDao">

    <resultMap id="BaseResultMap" type="com.TypeApi.entity.Reward_log" >
        <result column="id" property="id" />
        <result column="name" property="name" />
        <result column="reward_id" property="reward_id" />
        <result column="description" property="description" />
        <result column="uid" property="uid" />
        <result column="expired" property="expired" />
        <result column="created" property="created" />
    </resultMap>

    <sql id="Base_Column_List">
        `id`,
        `name`,
        `reward_id`,
        `description`,
        `uid`,
        `expired`,
        `created`
    </sql>

    <!-- 插入数据 -->
    <insert id="insert" parameterType="com.TypeApi.entity.Reward_log">
        INSERT INTO ${prefix}_reward_log
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test ='null != id'>
                `id`,
            </if>
            <if test ='null != name'>
                `name`,
            </if>
            <if test ='null != reward_id'>
                `reward_id`,
            </if>
            <if test ='null != description'>
                `description`,
            </if>
            <if test ='null != uid'>
                `uid`,
            </if>
            <if test ='null != expired'>
                `expired`,
            </if>
            <if test ='null != created'>
                `created`
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test ='null != id'>
                #{id},
            </if>
            <if test ='null != name'>
                #{name},
            </if>
            <if test ='null != reward_id'>
                #{reward_id},
            </if>
            <if test ='null != description'>
                #{description},
            </if>
            <if test ='null != uid'>
                #{uid},
            </if>
            <if test ='null != expired'>
                #{expired},
            </if>
            <if test ='null != created'>
                #{created}
            </if>
        </trim>
    </insert>


    <!-- 更新 -->
    <update id="update" parameterType="com.TypeApi.entity.Reward_log">
        UPDATE ${prefix}_reward_log
        <set>
            <if test ='null != name'>`name` = #{name},</if>
            <if test ='null != reward_id'>`reward_id` = #{reward_id},</if>
            <if test ='null != description'>`description` = #{description},</if>
            <if test ='null != uid'>`uid` = #{uid},</if>
            <if test ='null != expired'>`expired` = #{expired},</if>
            <if test ='null != created'>`created` = #{created}</if>
        </set>
        WHERE `id` = #{id}
    </update>

    <!-- 删除 -->
    <delete id="delete">
        DELETE FROM ${prefix}_reward_log
        WHERE `id` = #{key}
    </delete>

    <!-- 主键查询 -->
    <select id="selectByKey" resultMap="BaseResultMap">
        SELECT <include refid="Base_Column_List" />
        FROM ${prefix}_reward_log
        WHERE `id` = #{key}
    </select>

    <!-- 条件查询 -->
    <select id="selectList" resultMap="BaseResultMap">
        SELECT <include refid="Base_Column_List" />
        FROM ${prefix}_reward_log
        <where>
            <if test ='null != id'>
                and `id` = #{id}
            </if>
            <if test ='null != name'>
                and `name` = #{name}
            </if>
            <if test ='null != reward_id'>
                and `reward_id` = #{reward_id}
            </if>
            <if test ='null != description'>
                and `description` = #{description}
            </if>
            <if test ='null != uid'>
                and `uid` = #{uid}
            </if>
            <if test ='null != expired'>
                and `expired` = #{expired}
            </if>
            <if test ='null != created'>
                and `created` = #{created}
            </if>
        </where>
    </select>

    <!-- 分页条件查询 -->
    <select id="selectPage" resultMap="BaseResultMap">
        SELECT <include refid="Base_Column_List" />
        FROM ${prefix}_reward_log
        <where>
            <if test ='null != reward_log.id'>
                and `id` = #{reward_log.id}
            </if>
            <if test ='null != reward_log.name'>
                and `name` = #{reward_log.name}
            </if>
            <if test ='null != reward_log.reward_id'>
                and `reward_id` = #{reward_log.reward_id}
            </if>
            <if test ='null != reward_log.description'>
                and `description` = #{reward_log.description}
            </if>
            <if test ='null != reward_log.uid'>
                and `uid` = #{reward_log.uid}
            </if>
            <if test ='null != reward_log.expired'>
                and `expired` = #{reward_log.expired}
            </if>
            <if test ='null != reward_log.created'>
                and `created` = #{reward_log.created}
            </if>
            <if test ='"" != searchKey'>
                and CONCAT(IFNULL(`name`,'')) LIKE '%${searchKey}%'
            </if>
        </where>
        order by expired desc
        limit #{page,jdbcType=INTEGER}, #{pageSize,jdbcType=INTEGER}
    </select>

    <!-- 总量查询 -->
    <select id="total" resultType="java.lang.Integer">
        SELECT count(*) FROM ${prefix}_reward_log
        <where>
            <if test ='null != id'>
                and `id` = #{id}
            </if>
            <if test ='null != name'>
                and `name` = #{name}
            </if>
            <if test ='null != reward_id'>
                and `reward_id` = #{reward_id}
            </if>
            <if test ='null != description'>
                and `description` = #{description}
            </if>
            <if test ='null != uid'>
                and `uid` = #{uid}
            </if>
            <if test ='null != expired'>
                and `expired` = #{expired}
            </if>
            <if test ='null != created'>
                and `created` = #{created}
            </if>
        </where>
    </select>
</mapper>